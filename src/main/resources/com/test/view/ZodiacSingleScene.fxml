<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="360.0" styleClass="back" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.test.controller.ZodiacSingleController">
   <children>
      <Label alignment="CENTER" layoutX="83.0" layoutY="51.0" prefHeight="58.0" prefWidth="214.0" text="ProZodiac" textFill="#162cbc">
         <font>
            <Font name="Consolas Bold Italic" size="37.0" />
         </font>
      </Label>
      <Button fx:id="newProfileButton" layoutX="22.0" layoutY="257.0" mnemonicParsing="false" onAction="#onClickNewProfileButton" prefHeight="30.0" prefWidth="142.0" styleClass="style1" stylesheets="@css/button.css" text="Thêm Hồ sơ">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </Button>
      <ScrollPane hbarPolicy="NEVER" layoutX="20.0" layoutY="360.0" prefHeight="254.0" prefWidth="316.0" stylesheets="@style.css">
         <content>
            <VBox fx:id="profileListVBox" prefHeight="254.0" prefWidth="316.0" spacing="20.0" styleClass="back" stylesheets="@style.css">
               <padding>
                  <Insets top="1.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
      <Button fx:id="backButton" layoutX="195.0" layoutY="257.0" mnemonicParsing="false" onAction="#onClickBackButton" prefHeight="30.0" prefWidth="142.0" styleClass="style1" stylesheets="@css/button.css" text="Quay lại">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </Button>
      <Label fx:id="choosedProfileLabel" alignment="CENTER" layoutX="22.0" layoutY="134.0" prefHeight="66.0" prefWidth="316.0" styleClass="style1" stylesheets="@css/button.css" textFill="WHITE" />
      <Button fx:id="showInformationButton" layoutX="20.0" layoutY="308.0" mnemonicParsing="false" onAction="#onClickShowInformationButton" prefHeight="30.0" prefWidth="316.0" styleClass="style1" stylesheets="@css/button.css" text="Xem thông tin">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
